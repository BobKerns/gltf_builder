{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/CVS": true,
        "**/.ruby-lsp": true,
        ".coverage": true,
        ".venv/*": true,
        "**/__pycache__": true,
        "**/.pytest_cache/": true,
        "**/node_modules/": true
    },
    "hide-files.files": [],
    "cSpell.words": [
    ],
    "python.testing.pytestArgs": [
        "test"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "python.analysis.inlayHints.pytestParameters": true,
    "testing.displayedCoveragePercent": "statement",
    "testing.coverageToolbarEnabled": true,
    "github.copilot.chat.generateTests.codeLens": true,
    "npm.enableRunFromFolder": true,
    "npm.packageManager": "npm",
    "cSpell.customDictionaries": {
        "custom-dictionary-workspace": {
            "name": ".cspell.txt",
            "path": "./.cspell.txt",
            "addWords": true,
            "scope": "workspace"
        }
    },
    "ruff.configuration": "",
    "python.analysis.inlayHints.functionReturnTypes": false,
    "search.exclude": {
        ".venv/": true,
        ".vscode/": true,
        "**/__pycache__/*": true,
        "**/node_modules/": true,
        "package-lock.json": true,
        "typings/": true,
        "uv.lock": true
    },
    "cSpell.workspaceRootPath": "${workspaceFolder}",
    "cSpell.dictionaries": [
        "git",
        "python",
        "game-development",
        "python-common"
    ]
}