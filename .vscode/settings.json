{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/CVS": true,
        "**/.ruby-lsp": true,
        ".clj-kondo/": true,
        ".coverage": true,
        ".lsp/": true,
        ".venv/{lib,share,.gitignore,CACHEDIR.TAG,pyvenv.cfg}": true,
        "**/__pycache__": true,
        "**/.pytest_cache/": true,
        "**/node_modules/": true,
        "dist/": true,
        "typings/": true
    },
    "hide-files.files": [],
    "cSpell.words": [
    ],
    "python.testing.pytestArgs": [
        "test"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "python.analysis.inlayHints.pytestParameters": true,
    "testing.displayedCoveragePercent": "statement",
    "testing.coverageToolbarEnabled": true,
    "github.copilot.chat.generateTests.codeLens": true,
    "npm.enableRunFromFolder": true,
    "npm.packageManager": "npm",
    "cSpell.customDictionaries": {
        "custom-dictionary-workspace": {
            "name": ".cspell.txt",
            "path": "./.cspell.txt",
            "addWords": true,
            "scope": "workspace"
        }
    },
    "ruff.configuration": "",
    "python.analysis.inlayHints.functionReturnTypes": false,
    "search.exclude": {
        ".venv/": true,
        ".vscode/": true,
        "**/__pycache__/*": true,
        "**/node_modules/": true,
        "package-lock.json": true,
        "typings/": true,
        "uv.lock": true
    },
    "cSpell.workspaceRootPath": "${workspaceFolder}",
    "cSpell.dictionaries": [
        "git",
        "python",
        "game-development",
        "python-common"
    ],
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "*.mmd": "${capture}.svg,${capture}.png,${capture}Thumbnail.png",
        "*.glb": "${capture}.gltf,${capture}.json,${capture.bin}",
        "*.gltf": "${capture}.json,${capture.bin}",
        "glTF_RGB_*.svg": "glTF_*px_${capture}.png",
        "pyproject.toml": "requirements.txt,uv.lock,.python-version",
        ".browse.pid": ".browse.*",
        ".gitignore": ".gitignore,.gitattributes",
        "WebGL_RGB_*.svg": "WebGL_*px_${capture}.png",
        "README.md": "*.md",
        "CONFIG": ".gitignore,.gitattributes,package.json,package.lock,.codeqlignore,.cspell.txt,.envrc,pyrightconfig.json,.readthedocs.yaml"
    },
    "explorer.fileNesting.expand": false,
    "npm.scriptExplorerExclude": [
        ".*|.*"
    ],
    "explorer.sortOrder": "foldersNestsFiles",
    "testExplorer.mergeSuites": false,
    "markdownlint.config": {

        "MD009": false

    }
}